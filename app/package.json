    "@umijs/fabric": "2.3.1",
    "axios": "^0.21.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.14.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "expo": "~40.0.0",
    "expo-splash-screen": "~0.8.1",
